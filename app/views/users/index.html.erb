<h4>All other users</h4>
<ul>
  <% @users.each do |user| %>
    <li><%= user.email %></li>
    <% if current_user.following?(user) %>
      <%= button_to "Unfollow", [user, :following_relationship], method: :delete %>
    <% else %>
      <%= button_to "Follow", [user, :following_relationship] %>
    <% end %>
  <% end %>
</ul>

